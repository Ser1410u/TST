@page
@model Reolort2Model
@{
    ViewData["Title"] = "Список товаров выбранной аптеки";
    ViewBag.RESTURL = Model.RESTURL;
}
<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
    <div class="bicont">
        <div class="onehalf">
            <h1 class="display-8">Аптека</h1>
            <div id="grid1"></div>
        </div>
        <div class="onehalf">
            <h1 class="display-8">Товары</h1>
            <div id="grid2"></div>
        </div>
    </div>

</div>
<script src="~/js/Report2.js" asp-append-version="true"></script>
<script>
    GURL = '@ViewBag.RESTURL';
</script>